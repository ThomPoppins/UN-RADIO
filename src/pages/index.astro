import BaseLayout from "../layouts/BaseLayout.astro";
import "../styles/global.css";
const pageTitle = "UN-RADIO";
---

<BaseLayout>
  <div class="section-island">
    <h1>{pageTitle}</h1>

    <p>
      Who can still remember those empty days UN-RADIO wasn't even thought of
      yet?
    </p>

    <div class="static-text-container">
      <h2>`UN-RADIO` AUDIOVISUAL TEASER LIST</h2>
    </div>

    <div class="static-text-container">
      <p class="sub-title-text">
        Juuuust a little teaser... nothing special, but you'll need to follow
        these steps precisely boy...
      </p>
    </div>

    <div class="static-text-container">
      <p class="">FIRST! Play that funky music white boy!</p>

      <div class="release">
        <iframe
          width="100%"
          height="300"
          scrolling="no"
          frameborder="no"
          allow="autoplay"
          src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/playlists/1738780644&color=%23ff5500&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true&visual=true"
