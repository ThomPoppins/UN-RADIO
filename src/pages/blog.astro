---
import BaseLayout from "../layouts/BaseLayout.astro";
("../components/release/ReleaseInformation.astro");
// import YoutuberDetails from "../components/blog/youtube/YoutuberDetails.astro";

const res = await fetch("http://localhost:3000/api/posts"); // http://localhost:4321/api/posts by default
const posts = await res.json();

const pageTitle = "PAYLOAD";
---

<BaseLayout pageTitle={pageTitle}>
  {
    posts.docs.map((post) => (
      <>
        <h2 set:html={post.title} />
        <p set:html={post.content2} />
      </>
    ))
  }
</BaseLayout>
